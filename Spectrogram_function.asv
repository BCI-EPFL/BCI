function [p_MI, power_ratio] = Spectrogram_function(thisEpoch,num_channel,win,noverlap,freq,fs,'power')

final_e

for i=1:size(thisEpoch.data,3)-1

    [~,f,t,p_baseline] = spectrogram(thisEpoch.data(:,num_channel,i),win,noverlap,freq,fs,'power');
    [~,f,t,p_MI] = spectrogram(thisEpoch.data(:,num_channel,i),win,noverlap,freq,fs,'power');
    
    power_avg_baseline = mean(p_baseline');
    
    power_ratio = power_MI./(power_avg_baseline');
    
end

end